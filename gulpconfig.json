{
	"browserSyncOptions": {
		"proxy": "localhost:8080/",
		"notify": false
	},
	"browserSyncWatchFiles": {
		"files": ["./css/*.min.css", "./js/*.min.js", "./**/*.php"]
	},
	"paths": {
		"js": "./js",
		"css": "./css",
		"img": "./img",
		"imgsrc": "./src/img",
		"sass": "./sass",
		"node": "./node_modules/",
		"bower": "./bower_components/",
		"dev": "./src",
		"dist": "./dist",
		"distprod": "./dist-product",
		"vendor": ""
	}
}
